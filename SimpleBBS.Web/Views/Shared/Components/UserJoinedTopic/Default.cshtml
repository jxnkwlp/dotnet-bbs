@model TopicListViewModel

<div class="panel panel-default"  Condition="@(Model.List.Count != 0)" >
    <div class="panel-heading">最近参与的话题  <a href="#" class="pull-right">查看更多</a> </div>
    <div class="panel-body topic-list">
        @foreach (var item in Model.List)
        {
            <text>
                <partial name="_TopicRow" model="item" />
            </text>
        }
         
    </div>
</div>
